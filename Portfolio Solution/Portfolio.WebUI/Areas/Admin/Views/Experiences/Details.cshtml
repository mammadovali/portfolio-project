@model Experience


<!-- Page-header start -->
<div class="page-header">
    <div class="row align-items-end">

        <div class="col">
            <div class="page-header-breadcrumb">
                <ul class="breadcrumb-title">
                    <li class="breadcrumb-item" style="float: left;">
                        <a asp-controller="Dashboard" asp-action="index"> <i class="feather icon-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item" style="float: left;">
                        <a asp-action="index">All Experiences</a>
                    </li>
                    <li class="breadcrumb-item" style="float: left;">
                        <span>Details</span>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>

<!-- Page-header end -->
<!-- Page-body start -->
<div class="page-body">
    <div class="card">
        <div class="card-block">

            <div>
                <div class="form-group row">
                    <label asp-for="Duration" class="col-sm-2 col-form-label"></label>
                    <div class="col-sm-10">
                        <div class="form-control">
                            @Model.Duration
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label asp-for="JobTitle" class="col-sm-2 col-form-label"></label>
                    <div class="col-sm-10">
                        <div class="form-control" style="overflow:auto; min-height:50px;">
                            @Model.JobTitle
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label asp-for="CompanyName" class="col-sm-2 col-form-label"></label>
                    <div class="col-sm-10">
                        <div class="form-control" style="overflow:auto; min-height:150px;">
                            @Model.CompanyName
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label asp-for="Location" class="col-sm-2 col-form-label"></label>
                    <div class="col-sm-10">
                        <div class="form-control" style="overflow:auto; min-height:50px;">
                            @Model.Location
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label asp-for="Details" class="col-sm-2 col-form-label"></label>
                    <div class="col-sm-10">
                        <div class="form-control" style="overflow:auto; min-height:150px;">
                            @Html.Raw(Model.Details)
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-12 d-flex justify-content-end">
                        <a asp-action="index" class="btn btn-info">Cancel</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Page-body end -->
